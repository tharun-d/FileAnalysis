<link href="~/TableStyle.css" rel="stylesheet" />
<br />

<table align="center">
    <tr>
        <th>
            Employee Name
        </th>
        <th>
            Date
        </th>
        <th>
            Hours Filled
        </th>
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <td>
                @item.EmployeeName
            </td>
            <td>
                @item.ActDate
            </td>
            <td>
                @item.HoursMentioned
            </td>
        </tr>

    }


</table>